{
    "name": "prasso/autoprohub",
    "description": "Auto Pro Hub package for Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Prasso\\AutoProHub\\": "src/",
            "Prasso\\AutoProHub\\Database\\Factories\\": "database/factories/",
            "Prasso\\AutoProHub\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prasso\\AutoProHub\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prasso\\AutoProHub\\AutoProHubServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
